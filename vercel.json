{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9" 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": ["POST"],
            "dest": "api/webhook.py"
        }
    ]
}